<?xml version="1.0" encoding="utf-8"?>
<com.android.szparag.saymyname.views.widgets.overlays.SaymynameFloatingWordsView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/view_floating_words"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:soundEffectsEnabled="true"
    android:splitMotionEvents="false"
    tools:background="@android:color/holo_blue_dark"
    >

  <com.android.szparag.saymyname.views.widgets.FloatingWordTextView
      android:id="@+id/textview_word_auxilliary_1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_weight="1"
      android:visibility="gone"
      app:type="auxiliary"
      tools:text="testtext"
      tools:visibility="visible"
      />
  <com.android.szparag.saymyname.views.widgets.FloatingWordTextView
      android:id="@+id/textview_word_auxilliary_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_weight="1"
      android:visibility="gone"
      app:type="auxiliary"
      tools:text="testtext"
      tools:visibility="visible"
      />
  <com.android.szparag.saymyname.views.widgets.FloatingWordTextView
      android:id="@+id/textview_word_auxilliary_3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_weight="1"
      android:visibility="gone"
      app:type="auxiliary"
      tools:text="testtext"
      tools:visibility="visible"
      />

  <com.android.szparag.saymyname.views.widgets.FloatingWordTextView
      android:id="@+id/textview_word_primary_1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_weight="1"
      app:type="primary"
      android:visibility="gone"
      tools:text="testtext"
      tools:visibility="visible"
      />

  <com.android.szparag.saymyname.views.widgets.FloatingWordTextView
      android:id="@+id/textview_word_primary_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_weight="1"
      app:type="primary"
      android:visibility="gone"
      tools:text="testtext"
      tools:visibility="visible"
      />

  <com.android.szparag.saymyname.views.widgets.FloatingWordTextView
      android:id="@+id/textview_word_primary_3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_weight="1"
      app:type="primary"
      android:visibility="gone"
      tools:text="testtext"
      tools:visibility="visible"
      />

</com.android.szparag.saymyname.views.widgets.overlays.SaymynameFloatingWordsView>